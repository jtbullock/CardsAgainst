{
  "name": "CardsAgainst",
  "version": "0.0.0",
  "license": "MIT",
  "ignore": [
    "**/.*",
    "node_modules",
    "bower_components",
    "test",
    "tests"
  ],
  "dependencies": {
    "angular-route": "~1.4.7",
    "angular": "~1.4.7",
    "angular-socket-io": "~0.7.0",
    "bootstrap": "~3.3.5",
    "socket.io-client": "~1.3.7"
  },
  "overrides": {
    "bootstrap": {
      "main": [
        "dist/css/bootstrap.min.css",
        "dist/js/bootstrap.min.js",
        "dist/fonts/**/*"
      ]
    },
    "angular": {
      "main": "angular.min.js"
    },
    "angular-route": {
      "main": "angular-route.min.js"
    },
    "angular-socket-io": {
      "main": "socket.min.js",
      "dependencies": {
        "angular": "*",
        "socket.io-client": "*"
      }
    },
    "jquery": {
      "main": "dist/jquery.min.js"
    },
    "socket.io-client": {
      "main": "socket.io.js"
    }
  }
}
